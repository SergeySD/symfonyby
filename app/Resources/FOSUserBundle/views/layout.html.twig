{% extends '::layout.html.twig' %}
{% block title %}Сообщество Symfony программистов в Беларуси{% endblock %}

{% block body %}
    {% for key, message in app.session.getFlashes() %}
        <div class="{{ key }}">
            {{ message|trans({}, 'FOSUserBundle') }}
        </div>
    {% endfor %}
    {% block fos_user_content %}{% endblock %}
{% endblock %}

{% block menu %}
    {% render "SfbyStaticBundle:Content:menu" with {'active': 'login'} %}
{% endblock %}  