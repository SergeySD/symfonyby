{% form_theme form theme%}
<div class="articles">
    <h3>Регистрация</h3>
    <div class="form">
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
            {{ form_widget(form) }}
            <div>
                <a href="{{path('homepage')}}" class="gray_submit"><span>{{ 'registration.cancel'|trans({}, 'FOSUserBundle') }}</span></a>
                <span class="submit"><input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}"></span>
            </div>
        </form>
    </div>
</div>